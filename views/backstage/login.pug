html
    head
        title test
        meta(
        http-equiv="content-Type"
        content="text/html;charset=gb2312"
        )
        meta(
        http-equiv="X-UA-Compatible"
        content="IE=9;IE=8;IE=7;IE=EDGE"
        )
        meta(
        http-equiv="pragma"
        content="no-cache"
        )
        meta(
        http-equiv="Cache-Control"
        content="no-cache，must-revalidate"
        )
        meta(
        http-equiv="expires"
        content="0"
        )
        link(
        href="./../layui/css/layui.css"
        rel="stylesheet"
        type='text/css'
        )
        link(
        href="./../font-awesome-4.7.0/css/font-awesome.css"
        rel="stylesheet"
        type='text/css'
        )
        link(
        href="./../stylesheets/backstage/login.css"
        rel="stylesheet"
        type='text/css'
        )
        script(src="./../layui/layui.js")
        script(src="./../javascripts/lib/jquery-3.3.1.min.js")
        script(src="./../javascripts/utils/MyUtils.js")
        script(src="./../javascripts/backstage/login.js")
    body(class='layui-row')
        div(class='layui-col-xs4 layui-col-sm4 layui-col-md4 layui-col-lg4')
            div(style='width: 100%;height: 100%;')
        div(class='s_login layui-col-xs4 layui-col-sm4 layui-col-md4 layui-col-lg4')
            div(class='s_login_div')
                h1 后台登录
                    div(class='layui-form-item s_login_div_item')
                        label(class='layui-form-label') 用户名:
                        div(class="layui-input-block")
                            input(type="text" id="userName" name="userName" lay-verify="title" autocomplete="off" placeholder="请输入用户名" class="layui-input")
                    div(class='layui-form-item s_login_div_item')
                        label(class='layui-form-label') 密  码:
                        div(class="layui-input-block")
                            input(type="password" id="passWord"  name="passWord" lay-verify="title" autocomplete="off" placeholder="请输入密码" class="layui-input")
                    div(class='layui-form-item')
                        button(class='layui-btn layui-btn-normal'  id="s_login") 登录
        div(class='layui-col-xs4 layui-col-sm4 layui-col-md4 layui-col-lg4')
            div(style='width: 100%;height: 100%;')

